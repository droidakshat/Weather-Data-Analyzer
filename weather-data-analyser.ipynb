   "source": [
    "import pandas as pd\n",
    "\n",
    "# Load CSV\n",
    "df = pd.read_csv(\"weather.csv\")\n",
    "\n",
    "# Convert date column\n",
    "df['date'] = pd.to_datetime(df['date'], dayfirst=True)\n",
    "\n",
    "# Handle missing values  \n",
    "df.fillna(df.mean(numeric_only=True), inplace=True)"
   ]
